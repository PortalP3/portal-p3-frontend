{
  "root": "dist/",
  "https_only": true,
  "routes": {
    "/assets/*": "/assets/",
    "/**": "index.html"
  },
  "proxies": {
    "/api/": {
      "origin": "https://abacaxi-p3-api.herokuapp.com/index.php/wp-json/wp/v2"
    }
  },
  "headers": {
    "/": {
      "Cache-Control": "no-store, no-cache"
    },
    "**.js": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "cdn-instance.example.com"
    },
    "**.css": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "cdn-instance.example.com"
    }
  }
}
