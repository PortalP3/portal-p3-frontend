{
  "root": "dist/",
  "https_only": true,
  "routes": {
    "/assets/images/*": "/assets/images/",
    "/**": "index.html"
  },
  "proxies": {
    "/api/rating/": {
      "origin": "https://abacaxi-backend.herokuapp.com/wp-admin/admin-ajax.php?action=rate_post"
    },
    "/api/": {
      "origin": "https://abacaxi-backend.herokuapp.com/index.php/wp-json/wp/v2"
    }
  },
  "headers": {
    "/": {
      "Cache-Control": "no-store, no-cache"
    },
    "**.js": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "cdn-instance.example.com"
    },
    "**.css": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "cdn-instance.example.com"
    }
  }
}
