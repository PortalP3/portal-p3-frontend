{
  "name": "portal-p3-frontend",
  "version": "0.0.1",
  "main": "src/index.js",
  "license": "MIT",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js"
  },
  "scripts": {
    "build:dev": "webpack --progress --colors -d --env=dev",
    "build:uat": "webpack --progress --colors -d --env=uat",
    "build:prod": "webpack --define process.env.NODE_ENV='\"production\"' --progress --colors --env=prod",
    "start": "webpack-dev-server --port 8080 --progress --colors -d --env=dev",
    "lint-js": "eslint --config .eslintrc.json ./src",
    "lint-css": "yarn run stylelint src/**/*.scss src/**/**/*.scss --syntax scss",
    "test": "jest --coverage",
    "functional-test": "mocha -c --timeout 90000 --require babel-register --require test/hooks/css-modules-compiler.js --require babel-polyfill -r ./test/.setup.js --recursive ./test/functional",
    "test:watch": "yarn test -- --watch",
    "postinstall": "postinstall-build dist --script build:prod"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^22.2.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "css-loader": "^0.28.7",
    "css-modules-require-hook": "^4.0.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.5.0",
    "eslint-plugin-jasmine": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^22.2.1",
    "jsdom": "^11.1.0",
    "moxios": "^0.4.0",
    "node-sass": "^4.5.3",
    "nyc": "^11.1.0",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.0",
    "stylelint": "^8.0.0",
    "stylelint-config-css-modules": "^1.1.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.7.0",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "history": "^4.6.3",
    "postinstall-build": "^5.0.1",
    "prop-types": "^15.5.9",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^16.0.0",
    "react-g-analytics": "^0.4.2",
    "react-redux": "^5.0.6",
    "react-render-html": "^0.6.0",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "selenium-webdriver": "^3.5.0",
    "uuid": "^3.1.0",
    "yarn": "^1.0.0"
  }
}
